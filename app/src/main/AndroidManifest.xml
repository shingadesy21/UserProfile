<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.userprofile">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/custome_theme">
        <activity android:name=".Intero_Screen.Welcome_Intro"></activity>
        <activity android:name=".LogIn"
            android:theme="@style/AppTheme"
            android:label="LOGIN"/> <!-- <activity android:name=".User_Registration"></activity> -->
        <activity android:name=".User_Registration"
            android:theme="@style/AppTheme"
            android:label="REGISTRATION"/>
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme"
            android:label="PROFILE"/>
        <activity android:name=".Splash_Screen.SplashScreen1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />


                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>